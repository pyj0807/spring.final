<?xml version="1.0" encoding="UTF-8" ?> <!DOCTYPE mapper  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="timetable">
	<select id="getMovieTime" resultType="java.util.HashMap" parameterType="java.lang.String">
		select * from timetable where movie like #{movie} order by times asc
	</select>
	
	


</mapper>
	